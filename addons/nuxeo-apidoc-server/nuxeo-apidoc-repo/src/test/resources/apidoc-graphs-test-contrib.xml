<?xml version="1.0"?>
<component name="org.nuxeo.apidoc.test.graphs" version="1.0">

  <!-- override to lower timeout on force layouts -->
  <require>org.nuxeo.apidoc.snapshot.SnapshotManagerComponent</require>

  <extension target="org.nuxeo.apidoc.snapshot.SnapshotManagerComponent" point="graphs">
    <graph name="gephi" class="org.nuxeo.apidoc.introspection.graph.GephiGraphGeneratorImpl">
      <properties>
        <property name="forceLayout.timeout">10</property>
      </properties>
    </graph>
    <graph name="bundles" class="org.nuxeo.apidoc.introspection.graph.BundleGraphGeneratorImpl">
      <properties>
        <property name="forceLayout.timeout">10</property>
      </properties>
    </graph>
  </extension>

</component>
